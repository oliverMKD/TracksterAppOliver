<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:background="@color/transparent_gray"
        android:layout_height="match_parent">

    <include layout="@layout/toolbar_select_manufactor"
             android:id="@+id/include"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_10"
            android:scaleType="center"
            android:nestedScrollingEnabled="true"
            android:layout_marginStart="55dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/include"
            android:id="@+id/circleImageView1"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:scaleType="center"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_22"
            android:nestedScrollingEnabled="true"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/include"
            app:layout_constraintStart_toEndOf="@+id/circleImageView1"
            android:layout_marginStart="52dp"
            android:layout_marginEnd="55dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView7"
            app:layout_constraintHorizontal_bias="0.09"
            android:id="@+id/imageView11"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:scaleType="center"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_23"
            android:nestedScrollingEnabled="true"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/include"
            android:layout_marginEnd="55dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView7"
            android:layout_marginBottom="55dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView9"/>


    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_21"
            android:nestedScrollingEnabled="true"
            android:layout_marginStart="55dp"
            android:scaleType="center"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView5" android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/circleImageView1"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_11"
            android:nestedScrollingEnabled="true"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_marginStart="55dp"
            android:scaleType="center"
            android:id="@+id/imageView6"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView11"
            android:layout_marginEnd="55dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView9"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:scaleType="center"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_18"
            android:nestedScrollingEnabled="true"
            android:layout_marginEnd="55dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView9"
            app:layout_constraintTop_toBottomOf="@id/imageView7"
            android:layout_marginTop="55dp"/>


    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_16"
            android:nestedScrollingEnabled="true"
            android:layout_marginStart="55dp"
            app:layout_constraintStart_toStartOf="parent"
            android:scaleType="center"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView5"
            android:id="@+id/imageView8"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_20"
            android:nestedScrollingEnabled="true"
            android:scaleType="center"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" app:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_marginStart="55dp" android:id="@+id/imageView10"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_12"
            android:nestedScrollingEnabled="true"
            android:layout_marginEnd="55dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:scaleType="center"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:layout_constraintStart_toEndOf="@+id/imageView10"
            android:layout_marginStart="55dp" android:id="@+id/imageView12"/>


    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_19"
            android:nestedScrollingEnabled="true"
            android:layout_marginStart="55dp"
            app:layout_constraintStart_toStartOf="parent"
            android:scaleType="center"
            android:layout_marginTop="55dp" app:layout_constraintTop_toBottomOf="@+id/imageView8"
            android:layout_marginEnd="55dp" app:layout_constraintEnd_toStartOf="@+id/imageView14"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_17"
            android:nestedScrollingEnabled="true"
            android:scaleType="center"
            android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView10"
            android:layout_marginEnd="55dp" app:layout_constraintEnd_toStartOf="@+id/imageView13"
            android:id="@+id/imageView14"/>

    <ImageView
            android:layout_width="62dp"
            android:layout_height="62dp"
            android:background="@drawable/ellipse_1"
            android:src="@drawable/layer_20"
            android:nestedScrollingEnabled="true"
            android:layout_marginEnd="55dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:scaleType="center"
            app:layout_constraintStart_toEndOf="@+id/imageView10"
            android:layout_marginStart="55dp" app:layout_constraintHorizontal_bias="0.6"
            android:layout_marginTop="55dp" app:layout_constraintTop_toBottomOf="@+id/imageView12"
            android:id="@+id/imageView13"/>

    <Button android:layout_width="280dp" android:layout_height="45dp"
            android:id="@+id/select_manufactor"
            android:background="@drawable/login_btn_style"
            android:text="OTHERS"
            android:textSize="12sp"
            android:textAllCaps="true"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView14"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="55dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="55dp"
            app:layout_constraintEnd_toEndOf="parent"/>

</android.support.constraint.ConstraintLayout>